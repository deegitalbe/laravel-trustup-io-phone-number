{
    "name": "deegital/laravel-trustup-io-phone-number",
    "description": "A trustup package to format phone number.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Axel Marquet",
            "email": "axel.marquet@trustup.be"
        }
    ],
    "require": {
        "henrotaym/laravel-package-versioning-config": "^1.3",
        "giggsey/libphonenumber-for-php": "^8.13"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Deegital\\LaravelTrustupIoPhoneNumber\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Deegital\\LaravelTrustupIoPhoneNumber\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Deegital\\LaravelTrustupIoPhoneNumber\\Providers\\LaravelTrustupIoPhoneNumberServiceProvider"
            ]
        }
    }
}
